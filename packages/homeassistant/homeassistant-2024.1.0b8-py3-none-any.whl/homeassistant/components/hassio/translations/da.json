            "fix_flow": {
                "abort": {
                    "apply_suggestion_fail": "En eller flere af genstarterne mislykkedes. Tjek Supervisor-logfilerne for flere detaljer."
                },
                "step": {
                    "system_execute_rebuild": {
                        "description": "Standardkonfigurationen for tilf\u00f8jelsesprogrammer og Home Assistant er \u00e6ndret. For at opdatere konfigurationen med de nye standardindstillinger kr\u00e6ves en genstart for f\u00f8lgende:\n\n- {components}"
